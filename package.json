{
  "name": "echobot",
  "version": "1.0.0",
  "description": "express server",
  "main": "Server.js",
  "scripts": {
    "client": "cd echobotclient && yarn start",
    "server": "nodemon Server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
